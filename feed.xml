---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <title type="text" xml:lang="en">CircuitsOfImagination</title>
    <link type="application/atom+xml" href="{{ site.url }}feed.xml" rel="self"/>
    <link type="text" href="http://vitobotta.com" rel="alternate"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>{{ site.url }}</id>
    <author>
        <name>Nabil Alsharif</name>
    </author>
    <rights>Copyright (c) 2014-2015 Nabil Alsharif</rights>
    {% for post in site.posts limit:20 %}
    <entry>
        <title></title>
        <link href="{{ site.url }}{{ post.url }}"/>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <id>{{ site.url }}{{ post.url }}/</id>
        <summary type="html">{{ post.content | xml_escape }}</summary>
    </entry>
    {% endfor %}
</feed>
